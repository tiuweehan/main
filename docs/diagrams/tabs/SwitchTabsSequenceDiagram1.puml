@startuml
!include style.puml

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant "s:SwitchCommand" as SwitchCommand LOGIC_COLOR
participant ":CommandResult" as CommandResult LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
participant "g:GuiState" as GuiState MODEL_COLOR
participant "t:TabManager" as TabManager MODEL_COLOR
end box

LogicManager -> SwitchCommand : execute()
activate SwitchCommand

SwitchCommand -> Model : getGuiState()
activate Model

Model --> SwitchCommand : g
deactivate Model

SwitchCommand -> GuiState : getTabManager()
activate GuiState

GuiState --> SwitchCommand : t
deactivate GuiState

SwitchCommand -> TabManager : setDetailsTabPaneIndex(1)
activate TabManager

TabManager --> SwitchCommand
deactivate TabManager

create CommandResult
SwitchCommand -> CommandResult
activate CommandResult

CommandResult --> SwitchCommand
deactivate CommandResult

SwitchCommand --> LogicManager : result
deactivate SwitchCommand
@enduml
